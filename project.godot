; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "SceneLoader",
"language": "GDScript",
"path": "res://source/tools/SceneLoader.gd"
} ]
_global_script_class_icons={
"SceneLoader": ""
}

[application]

config/name="BMJ"
run/main_scene="res://source/game/Game.tscn"
config/icon="res://icon.png"

[autoload]

Scene="*res://source/global/Scene.tscn"
Instance="*res://source/global/Instance.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/driver/driver_name="GLES2"
quality/2d/gles2_use_nvidia_rect_flicker_workaround=true
quality/intended_usage/framebuffer_allocation=1
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
